/**app.wxss**/

page {
  height: 100%;
  width: 100%;
  background: #f3f4f5;
  font-size: 30rpx;
}

.navigator-hover {
  background-color: #fff;
}

::-webkit-scrollbar {
  height: 0;
  width: 0;
  color: transparent;
  opacity: 0;
}

::-webkit-scrollbar-thumb {
  height: 0;
  width: 0;
  box-shadow: inset 0 0 0 rgba(0, 0, 0, 0);
  -webkit-box-shadow: inset 0 0 0 rgba(0, 0, 0, 0);
}

::-webkit-scrollbar-track {
  height: 0;
  width: 0;
  box-shadow: inset 0 0 0 rgba(0, 0, 0, 0);
  -webkit-box-shadow: inset 0 0 0 rgba(0, 0, 0, 0);
}

.tab {
  display: flex;
  position: fixed;
  z-index: 2;
  top: 0;
  left: 0;
  width: 750rpx;
  height: 74rpx;
  background-color: #fff;
}

.tab-item {
  position: relative;
  flex: 1;
  z-index: 1;
  height: 74rpx;
  line-height: 74rpx;
  text-align: center;
  font-size: 28rpx;
  color: #666;
}

.tab-item .tab-text {
  color: #666;
  display: inline-block;
  font-size: 28rpx;
  box-sizing: border-box;
}

.tab-item .active {
  color: #ee272b;
  border-bottom: 1px solid #ee272b;
}

.flex-item {
  display: -webkit-flex;
  display: flex;
}

.flex-1 {
  flex: 1;
  -webkit-box-flex: 1;
}

.space-between {
  justify-content: space-between;
}

.flex-around {
  justify-content: space-around;
}

/* 背景 */

.bg-black {
  background: #000;
}

.bg-primary {
  background: #ee272b;
}

.bg-light-primary {
  background-color: rgba(238, 39, 43, 0.7);
  opacity: 0.7;
}

.bg-white {
  background: #fff;
}

/*排列 */

flex-1 {
  flex: 1;
  -webkit-box-flex: 1;
}

/* 浮动 */

.float-r {
  float: right;
}

/* 字体 */

.font-12 {
  font-size: 12rpx;
}

.text-gray {
  color: gray;
}

.text-primary {
  color: #ee272b;
}

.text-light-gray {
  color: #666;
}

.text-lightly-gray {
  color: #999;
}

/* 边距*/

.margin-t-10 {
  margin-top: 10rpx;
}

.margin-t-20 {
  margin-top: 20rpx;
}

.margin-l-20 {
  margin-left: 20rpx;
}

.padding-20 {
  padding: 20rpx;
}

.padding-b-10 {
  padding-bottom: 10rpx;
}

.padding-b-30 {
  padding-bottom: 30rpx;
}

/* list */

.common-list {
  position: relative;
  background: #fff;
}

.list-item {
  position: relative;
  padding: 30rpx 20rpx;
}

.arrow-right:before {
  position: absolute;
  content: '';
  width: 14rpx;
  height: 14rpx;
  top: 50%;
  right: 20rpx;
  margin-top: -7rpx;
  background: transparent;
  border: 1px solid #ccc;
  border-top: none;
  border-right: none;
  z-index: 2;
  -webkit-border-radius: 0;
  border-radius: 0;
  -webkit-transform: rotate(-135deg);
  transform: rotate(-135deg);
}

.arrow-right.free:before {
  color: #ee272b;
  border: 1px solid #ee272b !important;
}

.common-btn {
  box-sizing: border-box;
  border-radius: 17px;
  height: 27px;
  line-height: 27px;
  padding: 0 16px;
  font-size: 14px;
  background: #ee272b;
  color: #fff;
  text-align: center;
}

.common-btn.disabled {
  background: #eee !important;
  border: 1px solid #ddd !important;
  color: #ccc !important;
}

.btn-primary {
  background: #ee272b;
  color: #fff;
  border-color: transparent;
  border-radius: 17px;
  position: relative;
}

.btn-outline-primary {
  height: 24px;
  line-height: 23px;
  border: 1px solid #ee272b;
  background: transparent;
  color: #ee272b;
  font-size: 12px;
  padding: 0 10rpx;
  box-sizing: border-box;
}

.btn-outline-blue {
  height: 22px;
  line-height: 22px;
  border: 1px solid #1d98f3;
  background: transparent;
  color: #1d98f3;
  font-size: 12px;
  padding: 0 10px;
  box-sizing: border-box;
}

.btn-outline-dark {
  border: 1px solid #000;
  color: #000;
  background: transparent;
}

.btn-lg {
  height: 35px;
  line-height: 35px;
}

.btn-group {
  display: flex;
}

.btn-group .btn:not(:last-child) {
  border-top-right-radius: 0;
  border-bottom-right-radius: 0;
}

.btn-group .btn:not(:first-child) {
  border-right: 1px solid #ee272b;
  border-top-left-radius: 0;
  border-bottom-left-radius: 0;
}

.btn-group .btn-primary:not(:last-child)::after {
  content: ' ';
  position: absolute;
  right: -1px;
  top: -1px;
  bottom: -1px;
  width: 1px;
  background: #fff;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleX(0.5);
  transform: scaleX(0.5);
  z-index: 1;
}

.scroll-view {
  height: 100%;
}

.empty-tips {
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  display: -webkit-flex;
  align-items: center;
  text-align: center;
  justify-content: space-around;
}

.empty-tips .empty-content {
  text-align: center;
}

.empty-tips .empty-content image {
  margin: 0 auto;
  display: block;
}

.empty-tips image {
  height: 233rpx;
  width: 268rpx;
}

.empty-tips .empty-content .common-empty-img {
  height: 97rpx;
  width: 167rpx;
}

.empty-tips .empty-content .empty-text {
  display: inline-block;
  margin-top: 20rpx;
  color: #b9b9b9;
}

/* loading */

.loading {
  position: relative;
  height: 80rpx;
}

.loading .loading-bar {
  text-align: center;
  font-size: 28rpx;
  height: 80rpx;
  line-height: 80rpx;
  color: #ccc;
}

.loading .loading-img {
  display: inline-block;
  height: 80rpx;
  width: 80rpx;
  vertical-align: middle;
}

/* .loading .loaded::after {
  content: '';
  width: 100%;
  height: 1px;
  background: #ddd;
  position: absolute;
  left: 0;
  top: 50%;
  z-index: -1;
} */

.loading .loaded-text {
  width: 200rpx;
  display: inline-block;
  margin: 0 auto;
  z-index: 999;
  background: #f3f4f5;
}

.single-ellipsis {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.double-ellipsis {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

/*
* 热卖商品布局开始
*/

.hot-product-list {
  /* padding: 40rpx 0 0; */
  flex-wrap: wrap;
  justify-content: space-between;
}

.hot-product {
  width: 100%;
  position: relative;
  background: #f3f4f5;
  padding-bottom: 130rpx;
}

.buy-list {
}

.hot-title {
  padding: 10rpx;
  text-align: center;
  font-family: SimHei;
  line-height: 80rpx;
  color: #000;
}

.hot-icon {
  width: 22rpx;
  height: 28rpx;
  display: inline-block;
  vertical-align: middle;
  margin-right: 20rpx;
}

.hot-product-item {
  display: inline-block;
  width: 50%;
  margin-bottom: 0 20rpx;
  box-sizing: border-box;
  border-bottom: 10rpx solid #f2f2f2;
  background: #fff;
}

.hot-product-item:nth-child(2n-1) {
  border-right: 5rpx solid #f2f2f2;
}

.hot-product-item:nth-child(2n) {
  border-left: 5rpx solid #f2f2f2;
}

.hot-product-list .title {
  padding: 0 0 40rpx 0;
  width: 750rpx;
  height: 32rpx;
}

.hot-product-list .title .wraper {
  position: relative;
  width: 220rpx;
  height: 32rpx;
  margin: 0 auto;
}

.hot-product-list .title .wraper .icon {
  position: absolute;
  top: 4rpx;
  left: 0;
  width: 22rpx;
}

.hot-product-list .title .wraper .txt {
  position: absolute;
  top: -4rpx;
  left: 40rpx;
  width: 198rpx;
  font-size: 33rpx;
  color: #000;
}

.product-item {
  width: 50%;
  margin-bottom: 20rpx;
}

.product-item-inner {
  background: #fff;
  font-size: 0;
  /* border:0.5px solid #f2f2f2; */
}

.product-item-inner image {
  height: 240rpx;
  width: 100%;
}

.product-item-inner-l {
  margin-right: 5px;
}

.product-item-inner-r {
  margin-left: 5px;
}

.product-item-unit {
  font-size: 30rpx;
  line-height: 43rpx;
  color: #ee272b;
}

.product-price-nubmer {
  font-size: 40rpx;
  margin-left: 5rpx;
  line-height: 60rpx;
}

.product-item-title {
  height: 84rpx;
  line-height: 40rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  color: #000;
  font-size: 26rpx;
  letter-spacing: 0px;
}

.product-item-btn {
  width: 130rpx;
  height: 50rpx;
  line-height: 50rpx;
  background-color: #ee272b;
  border-radius: 25rpx;
  color: #fff;
  text-align: center;
  font-size: 30rpx;
}

.product-cover {
  display: block;
  width: 100%;
  height: 134px;
  margin-right: 10px;
  border-radius: 10px;
}

.product-item-info {
  padding: 10rpx 10rpx 20rpx;
  /* border-left:0.5px solid #f2f2f2;
  border-bottom:0.5px solid #f2f2f2;
  border-right:0.5px solid #f2f2f2; */
  box-sizing: border-box;
}

.product-item-sellinfo {
  font-size: 22rpx;
  color: #666;
}

.product-item-price {
  color: #ee272b;
}

.product-item-action {
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.product-item-action button, .product-item-action .btn {
  display: inline-block;
  width: 80px;
  height: 25px;
  line-height: 25px;
  background: #ee272b;
  text-align: center;
  color: #fff;
  font-size: 14px;
  border-radius: 10px;
  margin: 0;
}

/*
* 热卖商品布局结束
*/

.no-data {
  line-height: 100rpx;
  text-align: center;
}

button[open-type="share"] {
  outline: none;
  border: 0;
}

button::after {
  border: none;
}

/* 边框 */

.border, .border-t, .border-b, .border-l, .bordedr-r {
  position: relative;
}

.pixel-ratio-1 .border {
  border: 1rpx solid #eee;
}

.pixel-ratio-2 .border::after {
  content: '';
  display: block;
  position: absolute;
  left: 0;
  height: 100%;
  line-height: 100%;
  width: 100%;
  border: 1px solid #e3e3e3;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}

.pixel-ratio-2 .border-b::after {
  content: '';
  display: block;
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  border-bottom: 1px solid #e3e3e3;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}

.pixel-ratio-2 .border-t::after {
  content: '';
  display: block;
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  border-bottom: 1px solid #e3e3e3;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}

.pixel-ratio-3 .border::after {
  content: '';
  display: block;
  position: absolute;
  left: 0;
  bottom: 0;
  height: 100%;
  width: 100%;
  border: 1px solid #e3e3e3;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}

.pixel-ratio-3 .border-b::after {
  content: '';
  display: block;
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  border-bottom: 1px solid #e3e3e3;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}

.pixel-ratio-3 .border-t::after {
  content: '';
  display: block;
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  border-bottom: 1px solid #e3e3e3;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}

.radius-border {
  position: relative;
}

.pixel-ratio-2 .radius-border:before {
  content: "";
  pointer-events: none; /* 防止点击触发 */
  box-sizing: border-box;
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  border-radius: 8rpx;
  /* border: 1rpx solid #666; */
  box-shadow: 0 0 1rpx 0 rgb(211, 211, 211);
  -webkit-transform: (scale(0.5));
  -webkit-transform-origin: 0 0;
  transform: (scale(0.5));
  transform-origin: 0 0;
}

.pixel-ratio-3 .radius-border:before {
  content: "";
  pointer-events: none; /* 防止点击触发 */
  box-sizing: border-box;
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  border-radius: 8rpx;
  /* //border: 1rpx solid #666; */
  box-shadow: 0 0 1rpx 0 rgb(211, 211, 211);
  -webkit-transform: (scale(0.3));
  -webkit-transform-origin: 0 0;
  transform: (scale(0.3));
  transform-origin: 0 0;
}

.radius-border.active:before {
  box-shadow: 0 0 1rpx #f00;
}

/* 登陆 */

/*登录弹框*/

.dialog-mask {
  position: fixed;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.7);
  z-index: -1;
}

.dialog-box {
  position: fixed;
  z-index: 999;
  width: 100%;
  top: 0;
  left: 0;
  height: 100%;
  display: flex;
  display: -webkit-box-flex;
  align-items: center;
  animation: showdialog linear;
}

@keyframes showdialog {
  from {
    opacity: 0, ;
  }

  to {
    opacity: 1;
  }
}

.dialog-container {
  position: relative;
  width: 550rpx;
  min-height: 340rpx;
  max-height: 750rpx;
  margin: 0 100rpx;
  background: #fff;
  border-radius: 10rpx;
  overflow: hidden;
}

.dialog-content {
  min-height: 250rpx;
  position: relative;
  overflow-y: scroll;
}

.dialog-btn {
  width: 250rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  color: #ee272b;
  font-size: 32rpx;
  border-radius: 10rpx;
  border: 1px solid #ee272b;
}

.dialog-tips {
  padding: 20rpx 40rpx;
  font-size: 36rpx;
  line-height: 50rpx;
  letter-spacing: 0px;
  color: #666;
}

.close-box {
  position: absolute;
  bottom: -80rpx;
}

.dialog-title {
  width: 100%;
  font-size: 36rpx;
  font-weight: 600;
  line-height: 90rpx;
  text-align: center;
}

.modal-foot {
  position: absolute;
  bottom: 0;
  width: 100%;
  display: flex;
  display: -webkit-flex;
  justify-content: space-around;
}

.modal-foot .modal-foot-btn {
  height: 100rpx;
  line-height: 100rpx;
  flex: 1;
  text-align: center;
  padding: 0;
  font-size: 34rpx;
}

.modal-foot .modal-foot-btn.text-default {
  color: #666;
}

.modal-foot .modal-foot-btn.text-white {
  color: #fff;
}

.modal-foot .modal-foot-btn.text-primary {
  background: #ee272b;
  color: #fff;
  border-radius: 0;
}
